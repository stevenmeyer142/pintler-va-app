{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../tsc_out",
    "rootDir": ".",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules"]
}
