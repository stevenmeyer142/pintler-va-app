{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "../tsc_out",
    "rootDir": ".",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
  },
"include": ["./**/*.ts"],
"exclude": ["node_modules"]
}
